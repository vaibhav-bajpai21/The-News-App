<!DOCTYPE html>
<html lang="en">
<style>
    table,
    th,
    td {
        border: 1px solid black;
    }
</style>

<body class="container">


    <main>
        <div class="jumbotron">
            <h1>Feed Articles</h1>
            <ul>
                <table>
                    <tr>
                        <th>Article Image</th>
                        <th>Aritcle</th>
                    </tr>
                    <% res.forEach(function(element) { %>
                        <tr>
                            <td>
                                <img src=\<%=element.thumbnail_image %> alt="Thumbnail Image" width="200" height="100">
                            </td>
                            <td>
                                <strong><i>Headline: </i></strong>
                                <strong>
                                    <%= element.headline %>
                                </strong> <br>
                                <i>Category: </i>
                                <%= element.category %> <br>
                                    <i>Author Name: </i>
                                    <%= element.author_name %> <br>
                                <i>Uploaded At: </i>
                                <%= element.created_at %> <br>
                            </td>
                        </tr>

                        <% }); %>
                </table>
            </ul>
        </div>
    </main>

</body>

</html>